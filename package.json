  "scripts": {
    "testSuite1": "playwright test tests/finalTask.spec.ts --project=chromium --project=msedge --workers=2 --headed",
    "testSuite2": "playwright test tests/fT.notebooks.spec.ts --project=chromium --project=msedge --workers=2 --headed",
